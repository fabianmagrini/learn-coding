{
  "name": "aqs-service",
  "version": "1.0.0",
  "description": "Account Query Service - Core orchestration service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest tests/e2e --runInBand --detectOpenHandles",
    "test:e2e:watch": "jest tests/e2e --watch --runInBand",
    "test:pact:consumer": "jest tests/pact --testPathIgnorePatterns=provider-verification",
    "test:pact:provider": "jest tests/pact/provider-verification.test.ts",
    "test:pact": "npm run test:pact:consumer && npm run test:pact:provider"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.3",
    "@opentelemetry/exporter-prometheus": "^0.45.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "cockatiel": "^3.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "p-retry": "^6.2.0",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^16.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
